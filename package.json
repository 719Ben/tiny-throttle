    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^2.3.0",
    "prettier-eslint-cli": "^5.0.1",
    "rollup": "^2.50.5",
    "typescript": "^4.3.2",
    "uglify-js": "^3.13.8"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  }
